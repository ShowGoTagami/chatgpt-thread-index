{
  "manifest_version": 3,
  "name": "ChatGPT Question Navigator",
  "version": "1.0.0",
  "description": "Navigate through your ChatGPT questions with an easy-to-use sidebar",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["dist/content_script.js"],
      "css": ["src/styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
